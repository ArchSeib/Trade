<Page x:Class="Trade.AllPages.SecondMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Trade.AllPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SecondMenuPage">

    <Grid>
        <StackPanel Orientation="Vertical" VerticalAlignment="Center">
            <Button Width="200" Height="50" Margin="0 0 0 20" x:Name="BtnProducts" Click="BtnProducts_Click">Продукты</Button>
            <Button Width="200" Height="50" x:Name="BtnOrders" Click="BtnOrders_Click">Заказы</Button>
        </StackPanel>
    </Grid>
</Page>
